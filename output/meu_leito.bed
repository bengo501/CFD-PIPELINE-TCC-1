// arquivo .bed gerado pelo wizard web
// modo: blender_interactive

bed {
    diameter = 0.1 m;
    height = 0.2 m;
    wall_thickness = 0.002 m;
    clearance = 0.01 m;
    material = "steel";
}

lids {
    top_type = "flat";
    bottom_type = "flat";
    top_thickness = 0.003 m;
    bottom_thickness = 0.003 m;
    seal_clearance = 0.001 m;
}

particles {
    kind = "sphere";
    diameter = 0.005 m;
    count = 100;
    target_porosity = 0.4;
    density = 2500.0 kg/m3;
    restitution = 0.3;
    friction = 0.5;
    rolling_friction = 0.1;
    linear_damping = 0.1;
    angular_damping = 0.1;
    seed = 42;
}

packing {
    method = "rigid_body";
    gravity = -9.81 m/s2;
    substeps = 10;
    iterations = 10;
    damping = 0.1;
    rest_velocity = 0.01 m/s;
    max_time = 5.0 s;
    collision_margin = 0.001 m;
}

export {
    formats = ["stl_binary", "blend"];
    units = "m";
    scale = 1.0;
    wall_mode = "surface";
    fluid_mode = "none";
    manifold_check = true;
    merge_distance = 0.001 m;
}
